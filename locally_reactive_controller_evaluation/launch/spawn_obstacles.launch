<?xml version="1.0" encoding="utf-8"?>
<launch>

  <param name="obstacles_description"
          command="xacro --inorder '$(find locally_reactive_controller_evaluation)/models/obstacles.urdf.xacro'" />

  <node name="spawn_obstacles" pkg="gazebo_ros" type="spawn_model" 
        args="-urdf -param obstacles_description -model obstacles  -x 0 -y 0 -z 0.0" 
        respawn="false" output="screen" />

  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher">
    <remap from="robot_description" to="obstacles_description" />
  </node>

  <node pkg="tf" type="static_transform_publisher" name="obstacles_tf_broadcaster" args="0 0 0 0 0 0 map world 100" />


 </launch>